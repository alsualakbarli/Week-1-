import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        System.out.print("Enter 3 integers: ");
        int a = sc.nextInt();
        long b = sc.nextLong();
        float c = sc.nextFloat();

        float sum = a + b + c;
        System.out.println("Sum: " + sum);

    }
}
